<Window x:Class="AsynchronousES.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsynchronousES"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="downloadButton" Content="Download" HorizontalAlignment="Left" Margin="397,207,0,0" VerticalAlignment="Top" Width="75" Click="DownloadButton_Click"/>
        <TextBox x:Name="damageText" HorizontalAlignment="Left" Height="20" Margin="133,91,0,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top" Width="33"/>
        <Button x:Name="damageButton" Content="Apply damage" HorizontalAlignment="Left" Margin="186,91,0,0" VerticalAlignment="Top" Width="91"/>
        <TextBox x:Name="textHtmlMatches" HorizontalAlignment="Left" Height="23" Margin="512,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="270"/>
        <Button x:Name="buttonHtmlMatches" Content=".NET matches" HorizontalAlignment="Left" Margin="418,91,0,0" VerticalAlignment="Top" Width="75" Click="ButtonHtmlMatches_Click"/>
        <ProgressBar x:Name="progressBarMatches" HorizontalAlignment="Left" Height="10" Margin="418,116,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
